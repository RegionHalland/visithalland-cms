{
  "name": "regionhalland/visithalland",
  "type": "project",
  "license": "MIT",
  "description": "visithalland.com",
  "homepage": "https://www.visithalland.com/",
  "authors": [
    {
      "name": "Sebastian Marcusson",
      "email": "sebastian@digitaliseringsbyran.se",
      "homepage": "https://github.com/sebastiansson"
    },
    {
      "name": "Albin Martinsson",
      "email": "albin@digitaliseringsbyran.se",
      "homepage": "https://github.com/albinmartinsson91"
    }
  ],
  "keywords": [
    "wordpress", "visithalland", "region halland", "halland", "composer"
  ],
  "support": {
    "issues": "https://github.com/RegionHalland/visithalland-cms/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.6.9",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/sitepress-multilingual-cms",
        "version": "3.9.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6088&user_id=208567&subscription_key=a3484c13ba484b0fc8e9d14c7aa5e7c6&t=1520321072"
        }
      }
	},
	{
		"type": "package",
		"package": {
			"name": "wpml/wpml-string-translation",
			"version": "2.7.3",
			"type": "wordpress-plugin",
			"dist": {
				"type": "zip",
				"url": "https://wpml.org/?download=6092&user_id=208567&subscription_key=a3484c13ba484b0fc8e9d14c7aa5e7c6"
			}
		}
	},
	{
		"type": "package",
		"package": {
			"name": "wpml/acfml",
			"version": "0.7",
			"type": "wordpress-plugin",
			"dist": {
				"type": "zip",
				"url": "https://wpml.org/?download=1097589&user_id=208567&subscription_key=a3484c13ba484b0fc8e9d14c7aa5e7c6&t=1520321807"
			}
		}
	},
	{
		"type": "package",
		"package": {
			"name": "wpml/wpml-media-translation",
			"version": "2.3.3",
			"type": "wordpress-plugin",
			"dist": {
				"type": "zip",
				"url": "https://wpml.org/?download=7474&user_id=208567&subscription_key=a3484c13ba484b0fc8e9d14c7aa5e7c6&t=1520321807"
			}
		}
	}
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.4",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.9.4",
    "oscarotero/env": "^1.1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "philippbaschke/acf-pro-installer": "^1.0",
    "wpackagist-plugin/amp": "0.6.2",
    "wpackagist-plugin/autoptimize": "2.3.2",
    "wpackagist-plugin/better-search-replace": "1.3.2",
    "wpackagist-plugin/cache-enabler": "1.2.2",
    "wpackagist-plugin/ewww-image-optimizer": "4.1.0",
    "wpackagist-plugin/glue-for-yoast-seo-amp": "0.4.2",
    "wpackagist-plugin/redirection": "3.2.1",
    "wpackagist-plugin/regenerate-thumbnails": "3.0.2",
    "wpackagist-plugin/schema": "dev-trunk",
    "wpackagist-plugin/search-by-algolia-instant-relevant-results": "2.10.0",
    "yoast/wordpress-seo": "6.3.1",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpml/sitepress-multilingual-cms": "*",
    "wpml/wpml-string-translation": "*",
    "wpml/wpml-media-translation": "*",
    "regionhalland/visithalland-custom-post-types": "^1.0",
    "regionhalland/visithalland-custom-taxonomies": "^1.0"

  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
