{
  "name": "regionhalland/visithalland",
  "type": "project",
  "license": "MIT",
  "description": "visithalland.com",
  "homepage": "https://www.visithalland.com/",
  "authors": [
    {
      "name": "Sebastian Marcusson",
      "email": "sebastian@digitaliseringsbyran.se",
      "homepage": "https://github.com/sebastiansson"
    },
    {
      "name": "Albin Martinsson",
      "email": "albin@digitaliseringsbyran.se",
      "homepage": "https://github.com/albinmartinsson91"
    }
  ],
  "keywords": [
    "wordpress",
    "visithalland",
    "region halland",
    "halland",
    "composer"
  ],
  "support": {
    "issues": "https://github.com/RegionHalland/visithalland-cms/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.7.12",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/sitepress-multilingual-cms",
        "version": "4.2.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6088&user_id=208567&subscription_key=af2acb39560bf3897e4f0ba665033da2&t=1551257358&version=4.2.3"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-string-translation",
        "version": "2.10.2",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6092&user_id=208567&subscription_key=af2acb39560bf3897e4f0ba665033da2&t=1551257359&version=2.10.2"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/acfml",
        "version": "1.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=1097589&user_id=208567&subscription_key=af2acb39560bf3897e4f0ba665033da2&t=1551257360&version=1.1"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-media-translation",
        "version": "2.5.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=7474&user_id=208567&subscription_key=af2acb39560bf3897e4f0ba665033da2&t=1551257359&version=2.5.1"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.4",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "^4.9.4",
    "oscarotero/env": "^1.1.0",
    "roots/wp-password-bcrypt": "^1.0.0",
    "philippbaschke/acf-pro-installer": "^1.0",
    "wpackagist-plugin/autoptimize": "^2.3.2",
    "wpackagist-plugin/better-search-replace": "^1.3.2",
    "wpackagist-plugin/ewww-image-optimizer": "^4.1.0",
    "wpackagist-plugin/regenerate-thumbnails": "^3.0.2",
    "wpackagist-plugin/schema": "^1.7.1",
    "wpackagist-plugin/autodescription":"^3.1.3",
    "wpackagist-plugin/the-seo-framework-extension-manager":"^1.5.1",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpml/sitepress-multilingual-cms": "*",
    "wpml/wpml-string-translation": "*",
    "wpml/wpml-media-translation": "*",
    "regionhalland/visithalland-custom-post-types": "1.4.3",
    "regionhalland/visithalland-custom-taxonomies": "1.0.3",
    "guzzlehttp/guzzle": "^6.3",
    "wpackagist-plugin/wp-super-cache":"^1.6.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}